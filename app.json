{
  "expo": {
    "name": "avobusirusi",
    "slug": "avobusirusi",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    
    "plugins": [
      [
        "expo-barcode-scanner",
        {
          "cameraPermission": "Allow $(NSCameraUsageDescription) to access camera."
        }
      ]],

    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.busi.rusi",
      "config": {
      "googleMapsApiKey": "AIzaSyAl0LHyMqEQGkO6Fe69nqEasLPX_qim4dA"
      }
      },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
      
    }
  }
}
